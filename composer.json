{
  "name": "torann/laravel-asana",
  "description": "Asana API wrapper for Laravel",
  "keywords": ["laravel", "asana", "laravel 5"],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "torann@gmail.com"
    }
   ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "~5.5|~5.6|~5.7|~5.8|^6.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Torann\\LaravelAsana\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4-dev"
    },
    "laravel": {
      "providers": [
        "Torann\\LaravelAsana\\ServiceProvider"
      ],
      "aliases": {
        "Asana": "Torann\\LaravelAsana\\Facade\\Asana"
      }
    }
  }
}
